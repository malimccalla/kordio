// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Company {
  id           String  @id @default(uuid())
  name         String
  contactEmail String? @map("contact_email")
  location     String?
  website      String?
  description  String?

  @@map("companies")
}
